<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE html 
  PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<n:invisible xmlns:n="http://nevow.com/ns/nevow/0.1" n:render="inherit Skin.html">
<n:invisible n:pattern="title">Gallery</n:invisible>
<n:invisible n:pattern="admin" n:render="admin" />
<n:invisible n:pattern="headextra">
<link id="gallery" rel="alternate" href="/gallery/photos.rss" type="application/rss+xml" />
</n:invisible>

<n:invisible n:pattern="page-content">

    <div id="sidepanel">
      <h2 class="sifr">Gallery</h2>
      <n:invisible n:data="cmsitemselection name=Gallery Introduction" n:render="sequence" >  <n:invisible n:pattern="item" n:render="cmsitemselection name=Gallery Introduction" />  </n:invisible>
      <p class="piclens"><n:invisible n:data="cmsitemselection name=Piclens Text" n:render="sequence" >  <n:invisible n:pattern="item" n:render="cmsitemselection name=Piclens Text" />  </n:invisible>
      <a href="http://www.piclens.com"><img><n:attr name="src">/skin/images/piclens<n:invisible n:render="invert" />.gif</n:attr></img></a></p>
      <div class="search">
        <h3>search</h3>
        <div class="item">
          <p>Use the following form to search the gallery.</p>

        </div>
        <form action="/gallerysearch">
          <label>search term</label>
          <input id="text" name="q" type="text" />
          <input class="action" type="submit" value="click here to search" />
        </form>
      </div>
    </div>


    <div id="gallerycontent" class="clearfix">
<n:invisible n:render="galleries" />
    </div>



</n:invisible>
</n:invisible>


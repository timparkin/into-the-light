<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE html 
  PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<n:invisible xmlns:n="http://nevow.com/ns/nevow/0.1" n:render="inherit Skin.html">
<n:invisible n:pattern="title" n:render="phototitle" />
<n:invisible n:pattern="admin"><n:invisible n:render="admin" /></n:invisible>
<n:invisible n:pattern="headextra" />
<n:invisible n:pattern="page-content">

 
   

    <div id="category">
    <h2 class="sifr"><n:invisible n:render="category" /></h2>
    <h3 class="sifr"><n:invisible n:render="phototitle" /></h3>
    </div>
    <div id="price"><a href="#" onClick="$('.purchase').BlindToggleVertically(500, null, 'easeout');$(this).toggleClass('over');return false;">
      <img><n:attr name="src">/skin/images/button-price-off<n:invisible n:render="invert" />.gif</n:attr></img>
    </a></div>
    <div id="info"><a href="#" onClick="$('.record').BlindToggleVertically(500, null, 'easeout');$(this).toggleClass('over');return false;">
      <img><n:attr name="src">/skin/images/button-info-off<n:invisible n:render="invert" />.gif</n:attr></img>
    </a></div>

    <div id="sidepanel" class="photo clearfix">
    <n:invisible n:render="photoinformation" />
    </div>
    
    <div id="photo">
      <n:invisible n:data="products">
      <div id="previous"><n:invisible n:render="prev" /></div>
      <div id="up"><n:invisible n:render="up" /></div>
      <div id="next"><n:invisible n:render="next" /></div>
      </n:invisible>
      <div id="photobody">
      <n:invisible n:render="photo" />
      </div>
    </div>

</n:invisible>
</n:invisible>


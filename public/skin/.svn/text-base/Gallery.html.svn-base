<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE html 
  PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<n:invisible xmlns:n="http://nevow.com/ns/nevow/0.1" n:render="inherit Skin.html">
<n:invisible n:pattern="title">Gallery - <n:invisible n:render="gallerytitle" /></n:invisible>
<n:invisible n:pattern="admin" n:render="admin" />
<n:invisible n:pattern="headextra">
<link id="gallery" rel="alternate" href="photos.rss" type="application/rss+xml"><n:attr name="href"><n:invisible n:render="rssurl" /></n:attr></link>
</n:invisible>

<n:invisible n:pattern="page-content">

    <div id="sidepanel" class="gallery">
      <h2 class="sifr"><n:invisible n:render="gallerytitle" /></h2>
      <n:invisible n:render="gallerydescription" />
     <p class="piclens"><n:invisible n:data="cmsitemselection name=Piclens Text" n:render="sequence" >  <n:invisible n:pattern="item" n:render="cmsitemselection name=Piclens Text" />  </n:invisible>
      <a href="http://www.piclens.com"><img><n:attr name="src">/skin/images/piclens<n:invisible n:render="invert" />.gif</n:attr></img></a></p>
      <div class="search">
        <h3>other categories</h3>
        <div class="item">
          <n:invisible n:render="galleries" />
        </div>
      </div>
    </div>  


    <div id="gallerycontent" class="clearfix">
      <n:invisible n:data="products">
      <div id="previous"><n:invisible n:render="prev" /></div>
      <div id="up"><n:invisible n:render="up" /></div>
      <div id="next"><n:invisible n:render="next" /></div>
      </n:invisible>
<n:invisible n:render="gallery" />    
    </div>

  


</n:invisible>
</n:invisible>


<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
	
<n:invisible xmlns:n="http://nevow.com/ns/nevow/0.1">
  <h2>Photo Management</h2>
  <div>
    <p>Download a CSV of <a n:render="csv_link">all photos</a>.</p>
  </div>
  <p><a href="new" n:render="newProduct" class="linkButton">Add a Photograph</a></p>
  <h3 class="search">Search (click to open)</h3>  
  <form n:render="form search" />
  <form n:render="form inlineCategoryEdit" />
  <form action="" method="post" n:render="product_table_form">
    <n:invisible n:render="perPage" />
    <table n:render="product_table" />
    <div class="table_actions">
    <input type="submit" name="delete" value="Delete" />
    <input type="submit" name="updateCategories" value="Update Categories" />
    </div>
  </form>
  
</n:invisible>

